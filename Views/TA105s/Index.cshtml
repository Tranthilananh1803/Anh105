@model IEnumerable<Anh105.Models.TA105>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TAName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TAGener)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TAName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TAGener)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TAid">Sửa</a> |
                <a asp-action="Details" asp-route-id="@item.TAid">Chi tiết</a> |
                <a asp-action="Delete" asp-route-id="@item.TAid">Xóa</a>
            </td>
        </tr>
}
    </tbody>
</table>
